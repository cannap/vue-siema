{
  "name": "vue-siema",
  "description": "Lightweight and simple carousel for Vue 2.0",
  "version": "0.3.11",
  "author": "Marko Bolliger <marko@renka.de> (https://renka.de)",
  "private": true,
  "description": "Boilerplate for standalone Vue.js components",
  "keywords": [
    "vue",
    "slider",
    "carousel",
    "lightweight"
  ],
  "license": "MIT",
  "main": "dist/template-vue-component.cjs.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/cannap/vue-siema",
  "bugs": "https://github.com/cannap/vue-siema/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/cannap/vue-siema.git"
  },
  "scripts": {
    "prepublish": "npm run release",
    "clean": "del /Q -rf dist .tmp",
    "start": "webpack-dev-server --config scripts/webpack.config.example.js",
    "dev": "webpack-dashboard -- webpack-dev-server --inline --hot --no-info --config scripts/webpack.config.example.js",
    "build": "run-p build:cjs build:dev build:prod",
    "build:cjs": "rollup -c scripts/rollup.config.js --environment BUILD:cjs",
    "build:dev": "rollup -c scripts/rollup.config.js --environment BUILD:dev",
    "build:prod": "rollup -c scripts/rollup.config.js --environment BUILD:prod | uglifyjs -mc warnings=false --comments -o dist/template-vue-component.min.js",
    "build:example": "webpack --config scripts/webpack.config.example.js",
    "release": "run-s clean build"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.14.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.26.1",
    "eslint-plugin-html": "^1.5.3",
    "glob": "^7.0.3",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.0",
    "power-assert": "^1.3.1",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-vue": "^2.2.3",
    "testem": "^1.6.0",
    "uglifyjs": "^2.4.10",
    "vue-loader": "^10.0.2",
    "webpack": "^2.1.0-beta.25",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-server": "^2.1.0-beta.5"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "vue": "^2.1.8"
  }
}
